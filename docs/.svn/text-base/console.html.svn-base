<!DOCTYPE html>
<html>
<head>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
<style>
body{
background: #008000;
font-family: courier;
}

.container {
margin: 50px auto;
height: 600px;
width: 950px;
position:relative;
border:1px solid #fefefe;
}

#input {
width: 910px;
display:inline-block;
position:relative;
float:left;
}

.bottom {
position:absolute;
bottom:5px;
width: 100%;
}

.label {
width: 30px;
position: relative;
left: 15px;
top:5px;
color: #fefefe;
float:left;
}

.console {
position:absolute;
bottom:32px;
overflow:hidden;
}

.white{
color: #fefefe;
}

.blue{
color: #81bbd5;
}

.line {
margin-left: 15px;
margin-bottom:2px;
}

.margin {
margin-left: 40px;
margin-bottom:2px;
}

.cssConsole {
position:relative;
font-family: courier;
background: #008000;
color: #fefefe;
padding: 5px;
overflow: hidden;
height: 17px;
cursor:text;
}

.cssConsoleDisplay {
float: left; 
padding-left: 3px;
white-space: pre;
}

.cssConsoleInput {
position:absolute;
left:0px;
opacity: 0; 
} 

.cssConsoleCursor {
float: left;
background: #fefefe;
width:8px;
height:17px;
margin:0;
padding:0;
padding-right:1px;
padding-left:1px;
visibility: hidden;
}

.cssConsoleDisplay span.selected {
background: #fefefe;
color: #008000; 
}

.cssConsoleDisplay span {
background: #008000;
color: #fefefe; 
margin:0;
padding:0;
padding-right:1px;
padding-left:1px;
}

</style>
<meta charset=utf-8 />
<title>CSS console</title>
</head>
<body>
	<div class="container">
		<div class="console">
			<div class="line white">brap 1.0.0</div>
		</div>
		<div class="bottom">
			<div class="label">&gt;</div>
			<div id="input"></div>
		</div>
	</div>  

<script>
$.ajax({
  url: 'http://192.168.1.22:8888/f',
    success: function(_data) {
        {
            console.log ("AMERICA");
            var data = jQuery.parseJSON (_data);
            idiots = Object.keys(data['state']['whom']).join (", ");

            duh = $.map(data['public'], function(v, i)
            {
                return v['from'] == '' ? v['body'] : v['from'] + ": " + v['body'];
            });
            $('.console').append("<span class='line white'>Logged in: " + idiots + "<br/>" + "Subject: " + data['state']['subject'] + "<br/>" + duh.join ("<br/>") + "</span>");

            console.log ("FUCK YEAH");
        }
              }
              });
</script>

</body>
</html>
